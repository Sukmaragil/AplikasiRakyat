<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.aplikasirakyat">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PermohonanBelumMenikah"
            android:parentActivityName=".MainActivity"
            android:label="@string/belum_menikah">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
        <activity android:name=".PermohonanPenghasilanTidakTetap"
            android:label="@string/penghasilan_tidak_tetap"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
        <activity android:name=".PermohonanBelumMemilikiRumah"
            android:parentActivityName=".MainActivity"
            android:label="@string/belum_memiliki_rumah">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
        <activity android:name=".PermohonanTidakKeberatanTetangga"
            android:parentActivityName=".MainActivity"
            android:label="@string/tidak_keberatan_tetangga">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
        <activity android:name=".PermohonanIzinBerpergian"
            android:parentActivityName=".MainActivity"
            android:label="@string/izin_berpergian">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
        <activity android:name=".InformasiBelumMenikah"
            android:parentActivityName=".MainActivity"
            android:label="Informasi Pelayanan">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
        <activity android:name=".InformasiTidakKeberatanTetangga"
            android:parentActivityName=".MainActivity"
            android:label="Informasi Pelayanan">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
        <activity android:name=".InformasiBelumMemilikiRumah"
            android:parentActivityName=".MainActivity"
            android:label="Informasi Pelayanan">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
        <activity android:name=".InformasiIzinBerpergian"
            android:parentActivityName=".MainActivity"
            android:label="Informasi Pelayanan">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
        <activity android:name=".InformasiPenghasilanTidakTetap"
            android:parentActivityName=".MainActivity"
            android:label="Informasi Pelayanan">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
        <activity android:name=".LacakPermohonan"
            android:parentActivityName=".MainActivity"
            android:label="Lacak Permohonan">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
    </application>

</manifest>
